{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"slug": "text-title",
	"title": "Title",
	"blockTypes": [
		"core/heading",
		"core/paragraph",
		"core/navigation",
		"core/site-tagline",
		"core/site-title"
	],
	"styles": {
		"elements": {
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--theme-6)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--theme-6)"
				}
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--x-large)",
			"fontWeight": "550",
			"fontStyle": "normal",
			"letterSpacing": "normal"
		}
	}
}
