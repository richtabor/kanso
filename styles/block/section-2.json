{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"slug": "section-2",
	"title": "Style 2",
	"blockTypes": [ "core/group", "core/columns", "core/column", "tabor/card" ],
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--theme-2)",
			"text": "var(--wp--preset--color--theme-6)"
		},
		"border": {
			"radius": "var(--wp--preset--spacing--20)",
			"style": "solid",
			"width": "1px",
			"color": "var(--wp--preset--color--theme-3)"
		},
		"blocks": {
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--theme-6)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--theme-6)"
							}
						}
					}
				}
			}
		}
	}
}
